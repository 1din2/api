{
  "name": "1of2-api",
  "version": "0.0.1",
  "private": true,
  "description": "GraphQL OneOfTwo API",
  "author": "Dumitru Cantea <canteadumitru@gmail.com>",
  "engines": {
    "node": ">=16.13.1",
    "npm": "6.14.x"
  },
  "scripts": {
    "dev": "docker-compose up && nodemon",
    "test": "jest --maxWorkers=10 --reporters=default --useStderr --detectOpenHandles --verbose",
    "test-unit": "jest src/tests/unit --maxWorkers=10 --reporters=default --useStderr --detectOpenHandles test/unit",
    "test-integration": "jest src/tests/integration --maxWorkers=10 --reporters=default --useStderr --detectOpenHandles test/integration",
    "docker-test": "docker-compose up -d && npm test && docker-compose down",
    "docker-test-unit": "docker-compose up -d && yarn test-unit && docker-compose down",
    "docker-test-integration": "docker-compose up -d && yarn test-integration && docker-compose down",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "prettier": "./node_modules/.bin/prettier --write ./**/*.js",
    "check-dependencies": "./node_modules/.bin/depcheck",
    "copy-files": "./node_modules/.bin/cpx \"src/**/*.{json,html,csv,tsv,json,png,jpg}\" dist",
    "knex": "node dist/services/db/scripts/knex.js",
    "ts-knex": "ts-node src/services/db/scripts/knex.ts",
    "migrate": "yarn ts-knex migrate:latest",
    "rollback": "yarn ts-knex migrate:rollback",
    "new-migrate": "yarn ts-knex migrate:make",
    "new-seed": "yarn ts-knex seed:make",
    "seed": "yarn ts-knex seed:run",
    "drop-db": "node dist/services/db/scripts/drop-db.js",
    "drop-cache": "node dist/services/db/scripts/drop-cache.js",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn tsc --project ./tsconfig.prod.json && yarn copy-files",
    "test-build": "npm run clean && npm run tsc && npm run copy-files",
    "build:watch": "yarn clean && ./node_modules/.bin/tsc -w",
    "nodemon": "nodemon",
    "start": "node dist/index.js",
    "start-inside-docker": "ts-node src/",
    "prep-db": "yarn build && yarn migrate && yarn seed",
    "jest": "jest",
    "tsc": "./node_modules/.bin/tsc",
    "restart": "yarn clean && yarn build && npm start",
    "watch": "./node_modules/.bin/concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "debug": "./node_modules/.bin/concurrently \"tsc -w\" \"nodemon --watch src --nolazy --inspect=0.0.0.0:9222 --nolazy src/index.ts\"",
    "reset": "yarn build && yarn reset-db",
    "reset-db": "yarn drop-cache && yarn drop-db && yarn migrate && yarn seed",
    "generate-api-client": "generate-graphql-client --post -o ./src/tests/generated-api-client -e http://localhost:42201/graphql"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.9",
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "^3.6.2",
    "aws-sdk": "^2.1087.0",
    "axios": "^1.4.0",
    "class-validator": "^0.14.0",
    "content-type": "^1.0.5",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^16.0.0",
    "emittery": "^0.6.0",
    "express": "^4.17.2",
    "express-rate-limit": "^5.1.3",
    "graphql": "^15.3.0",
    "ioredis": "^4.16.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.19",
    "openai": "^3.2.1",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "pg": "^8.2.1",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.32.1",
    "slug": "^8.2.2",
    "type-graphql": "^1.1.1",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/content-type": "^1.1.5",
    "@types/debug": "^4.1.5",
    "@types/express-rate-limit": "^5.1.0",
    "@types/faker": "^4.1.11",
    "@types/ioredis": "^4.16.0",
    "@types/jest": "^25.2.1",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/passport": "^1.0.12",
    "@types/passport-facebook": "^2.1.11",
    "@types/pg": "^7.14.3",
    "@types/ramda": "^0.27.64",
    "@types/rimraf": "^3.0.2",
    "@types/slug": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "atonic": "^1.0.4",
    "concurrently": "^5.2.0",
    "cpx": "^1.5.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "faker": "^4.1.0",
    "graphql-typed-client": "^1.7.4",
    "jest": "^27.3.1",
    "jest-junit": "^10.0.0",
    "jest-matcher-utils": "^26.3.1",
    "lint-staged": "^9.4.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^8.5.0",
    "tslint": "^6.1.0",
    "typescript": "^4.5.5"
  }
}
